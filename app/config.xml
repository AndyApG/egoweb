<?xml version='1.0' encoding='utf-8'?>
<widget id="com.rand.egowebmoble" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>EgoWeb Mobile</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <preference name="permissions" value="none" />
    <preference name="BackupWebStorage" value="local" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <preference name="iosPersistentFileLocation" value="Library" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <preference name="android-targetSdkVersion" value="18" />
    <preference name="android-minSdkVersion" value="18" />
    <icon src="www/icons/icon.png" />
    <platform name="ios">
        <config-file parent="NSAppTransportSecurity" target="*-Info.plist">
            <array>
                <dict>
                    <key>NSAllowsArbitraryLoads</key>
                    <true />
                </dict>
            </array>
        </config-file>
        <icon height="1024" src="www/icons/icon-1024.png" />
        <icon height="76" src="www/icons/icon-76.png" width="76" />
        <icon height="152" src="www/icons/icon-76@2x.png" width="152" />
        <icon height="72" src="www/icons/icon-72.png" width="72" />
        <icon height="144" src="www/icons/icon-72@2x.png" width="144" />
        <icon height="167" src="www/icons/icon-83.5@2x.png" width="167" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <engine name="android" spec="^7.1.4" />
    <engine name="ios" spec="~4.5.5" />
</widget>
